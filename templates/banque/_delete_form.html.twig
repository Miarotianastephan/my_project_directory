<form method="post" action="{{ path('app_banque_delete', {'id': banque.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ banque.id) }}">
    <button title="Supprimer la banque" class="btn btn-label-danger">{{ ux_icon('lets-icons:trash-duotone',{height: '100%', width:'8%'}) }}</button>
</form>
